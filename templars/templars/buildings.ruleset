sound		= "w_generic"
sound_alt	= "-"
helptext 	= _("\
The Golden Temple, also known as Harimandir Sahib ('abode of god'), is\
 spiritually the most significant shrine in Sikhism. Completed in 1604,\
 the Golden Temple is an open house of worship for all people, from all\
 walks of life and faiths. Although destroyed several times by the Mughal
 and invading Afghan armies, the temple was always restored when peace\ 
 came to the state of Punjab, India.\
")

[building_grail_program]
name		= _("Grail Program")
genus		= "GreatWonder"
